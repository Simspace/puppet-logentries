{
  "name": "logentries-logentries",
  "version": "0.1.2",
  "author": "logentries",
  "summary": "Install and configure the logentries.com daemon",
  "license": "MIT",
  "source": "https://github.com/logentries",
  "project_page": "https://logentries.com/doc/linux-agent-deployment-via-puppet/",
  "issues_url": "https://forum.logentries.com/",
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 2.7.0 < 2015.3.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 2.7.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6.0",
        "7.0"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 2.6.0"}
  ]
}
